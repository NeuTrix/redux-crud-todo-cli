{
    "env": {
        "browser": true
    },
    "extends": "airbnb",
    "rules": {
        "indent": [
            "error",
            "tab"
        ], 
        "no-console": "off",
        "no-tabs": [
            "error", 
            { "allowIndentationTabs": true }
        ], 
        "react/jsx-filename-extension": [
            1, 
            { "extensions": [".js", ".jsx"] }
        ]
    }

}